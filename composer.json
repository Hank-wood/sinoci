{
  "name": "sinoci/sinoci",
  "description": "Simple yet powerful framework for building modern web sites.",
  "version": "0.0.0",
  "homepage": "https://github.com/sinoci/sinoci",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/sinoci/sinoci/issues",
    "wiki": "https://github.com/sinoci/sinoci/wiki"
  },
  "require": {
    "php": ">=5.4.0",
    "codeigniter/framework": "3.0.*",
    "illuminate/database": "5.0.*",
    "illuminate/pagination": "5.0.*"
  },
  "require-dev": {
    "illuminate/events": "5.0.*",
    "illuminate/view": "5.0.*",
    "leafo/scssphp": "0.6.*",
    "symfony/var-dumper": "2.8.*"
  },
  "autoload": {
    "files": [
      "resources/helpers/functions.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  },
  "config": {
    "process-timeout": 0
  },
  "scripts": {
    "serve": "php -S localhost:9000 -t public server.php",
    "guide": "php -S localhost:9000 -t vendor/codeigniter/framework/user_guide"
  }
}
